<?xml version="1.0" encoding="UTF-8"?>

<manifest>

	<default
		revision="warrior"/>

	<remote
		name="yoctoproject"
		fetch="git://git.yoctoproject.org"/>
	<remote
		name="openembedded"
		fetch="git://git.openembedded.org"/>
	<remote
		name="github"
		fetch="ssh://git@github.com"/>
	<remote
		name="phytec"
		fetch="git://git.phytec.de"/>

	<project
		name="poky.git"
		remote="yoctoproject"
		path="poky"/>
	<project
		name="meta-openembedded.git"
		remote="openembedded"
		path="meta-openembedded"/>
	<project
		name="meta-freescale.git"
		remote="yoctoproject"
		path="meta-freescale"/>
	<project
		name="meta-phytec.git"
		remote="phytec"
		path="meta-phytec"/>

</manifest>
