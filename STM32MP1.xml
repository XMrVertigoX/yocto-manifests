<?xml version="1.0" encoding="UTF-8"?>

<manifest>

	<default
		revision="thud"/>

	<remote
		name="yoctoproject"
		fetch="git://git.yoctoproject.org"/>
	<remote
		name="openembedded"
		fetch="git://git.openembedded.org"/>
	<remote
		name="github"
		fetch="ssh://git@github.com"/>

	<project
		name="poky.git"
		remote="yoctoproject"
		path="poky"/>
	<project
		name="meta-openembedded.git"
		remote="openembedded"
		path="meta-openembedded"/>
	<project
		name="STMicroelectronics/meta-st-stm32mp.git"
		remote="github"
		path="meta-st-stm32mp"/>
	<project
		name="STMicroelectronics/meta-st-openstlinux.git"
		remote="github"
		path="meta-st-openstlinux"/>

</manifest>
